  <minim:Requirement rdf:about="file:///usr/workspace/wf4ever-ro-manager/Checklists/runnable-wf-trafficlight/checklist.rdf#workflow_inputs_aggregated">
    <minim:isDerivedBy>
      <minim:ContentMatchRequirementRule rdf:nodeID="N53df6e00f0a1408eba84bdb9c2d1740f">
        <minim:aggregatesTemplate>
          {+if}
        </minim:aggregatesTemplate>
        <minim:forall>
          ?wf rdf:type wfdesc:Workflow ;
              wfdesc:hasInput [ wfdesc:hasArtifact ?if ] .
        </minim:forall>
        <minim:showfail>Workflow %(wf)s input %(if)s is not aggregated</minim:showfail>
        <minim:showpass>All declared workflow inputs are aggregated</minim:showpass>
        <minim:derives rdf:resource="file:///usr/workspace/wf4ever-ro-manager/Checklists/runnable-wf-trafficlight/checklist.rdf#workflow_inputs_aggregated"/>
      </minim:ContentMatchRequirementRule>
    </minim:isDerivedBy>
  </minim:Requirement>

